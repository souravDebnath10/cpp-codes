#include<iostream>
using namespace std;
int main(){
    int shaun1[] = {1,3,5,7,8,9,33};
    int shaun2[] = {2,4,1,6,8,9,11};
    int min1=shaun1[0],min2=shaun2[0];
    for(int i=0;i<7;i++){
       if(min1>shaun1[i]){
            min1=shaun1[i];
       }
    }
    cout<<min1<<endl;
    for(int j=0;j<7;j++){
        if(min2>shaun2[j]){
            min2=shaun2[j];
        }
    }
    cout<<min2<<endl;
    int sum = min1+min2;
    cout<<sum;
    return 0;
}
