#include <iostream>
using namespace std;
int main() {
    int i,j;
    cout<<"Enter first no: ";
    cin>>i;

    cout<<"Enter second no: ";
    cin>>j;

    char op;
    cout<<"Enter operator(+, -, *, /): ";
    cin>>op

    switch (op)
    {
    case '+':
        cout<<i<<op<<j<<" = "<<(i+j)<<endl;
        break;
    
    case '-':
        cout<<i<<op<<j<<" = "<<(i-j)<<endl;
        break;

    case '*':
        cout<<i<<op<<j<<" = "<<(i*j)<<endl;
        break;

    case '/':
        cout<<i<<op<<j<<" = "<<(i/j)<<endl;
        break;

    default:
        cout<<"Invalid operator"<<endl;
        break;
    }

    return 0;
}
