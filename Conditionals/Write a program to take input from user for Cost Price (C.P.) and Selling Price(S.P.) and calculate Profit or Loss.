# include <iostream>
using namespace std;
int main() {
    float cp,sp;
    cout<<"Enter cost price and selling price: ";
    cin>>cp>>sp;

    if(cp > sp) cout<<"Loss = "<<(cp - sp)<<endl;
    else if(sp > cp) cout<<"Profit = "<<(sp - cp)<<endl;
    else cout<<"Zero profit and loss";
    
    return 0;
}
